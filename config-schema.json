{
  "env": {
    "doc": "Application environment",
    "format": ["production", "development", "test"],
    "default": "production",
    "env": "NODE_ENV"
  },
  "log_level": {
    "doc": "Log level",
    "format": ["error", "warn", "info", "debug", "trace"],
    "default": "info"
  },
  "port": {
    "doc": "The port to bind",
    "format": "port",
    "default": 8018
  },
  "Instagram_ACCESS_TOKEN": {
    "doc": "NEED MORE INFO",
    "format": "*",
    "default": null
  },
  "Instagram_API_ENDPOINT": {
    "doc": "Instagram recent entries",
    "format": "*",
    "default": "https://api.instagram.com/v1/users/self/media/recent/",
  },
  "Pinboard_API_ENDPOINT": {
    "doc": "Pinboard recent entries",
    "format": "*",
    "default": null
  },
  "Pinboard_API_TOKEN": {
    "doc": "Pinboard API Token",
    "format": "*",
    "default": "https://api.pinboard.in/v1/posts/recent"
  }
}
