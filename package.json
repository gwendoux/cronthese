{
  "name": "cronthese",
  "version": "1.0.0",
  "description": "trying to backup some things from differents sources",
  "main": "app.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": " echo \"no test found. You should write tests\"",
    "prestart:watch": "npm run lint:fix",
    "start": "node app.js",
    "start:watch": "nodemon",
    "deploy:init": "pm2 ecosystem",
    "deploy:setup": "pm2 deploy ecosystem.json production setup",
    "deploy": "pm2 deploy ecosystem.json production",
    "postinstall": "npm prune",
    "export:db": "mkdirp ./backup/ && mongoexport --db coffee --collection coffees --out ./backup/coffee-coffees.json && mongoexport --db links --collection links --out ./backup/links-links.json  "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gwendoux/cronthese.git"
  },
  "keywords": [
    "cron",
    "node",
    "javascript"
  ],
  "author": "gwendoux",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gwendoux/cronthese/issues"
  },
  "homepage": "https://github.com/gwendoux/cronthese#readme",
  "dependencies": {
    "aws-bluebird": "1.0.0",
    "moment": "2.13.0",
    "mongoose": "4.5.3",
    "node-schedule": "1.1.1",
    "request-promise": "3.0.0"
  },
  "devDependencies": {
    "commander": "2.9.0",
    "convict": "1.4.0",
    "eslint": "2.13.1",
    "mkdirp": "0.5.1",
    "nodemon": "1.9.2",
    "pm2": "1.1.3",
    "tracer": "0.8.3"
  }
}
