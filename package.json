{
  "name": "cronthese",
  "version": "1.0.0",
  "description": "trying to backup some things from differents sources",
  "main": "app.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "posttest": "npm run lint",
    "unit-test": "NODE_ENV=test mocha test/test.js",
    "pretest": "rm -rf ./test/coverage",
    "test": "NODE_ENV=test istanbul cover _mocha test/test.js",
    "test:report": "open ./test/coverage/lcov-report/index.html",
    "prestart:watch": "npm run lint",
    "start": "node app.js",
    "start:watch": "nodemon",
    "deploy:init": "pm2 ecosystem",
    "deploy:setup": "pm2 deploy ecosystem.json production setup",
    "deploy": "pm2 deploy ecosystem.json production",
    "postinstall": "npm prune",
    "safefreeze": "rm -rf node_modules npm-shrinkwrap.json && npm install && npm dedupe && npm test && npm shrinkwrap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gwendoux/cronthese.git"
  },
  "keywords": [
    "cron",
    "node",
    "javascript"
  ],
  "author": "gwendoux",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gwendoux/cronthese/issues"
  },
  "homepage": "https://github.com/gwendoux/cronthese#readme",
  "dependencies": {
    "moment": "2.13.0",
    "mongoose": "4.5.0",
    "node-schedule": "1.1.0",
    "request-promise": "3.0.0"
  },
  "devDependencies": {
    "commander": "2.9.0",
    "convict": "1.4.0",
    "eslint": "2.12.0",
    "istanbul": "0.4.3",
    "mocha": "2.5.3",
    "must": "0.13.1",
    "nodemon": "1.9.2",
    "pm2": "1.1.3",
    "supertest": "1.2.0",
    "supertest-as-promised": "3.1.0",
    "tracer": "0.8.3"
  }
}
